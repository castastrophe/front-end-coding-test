{% extends "../_base.twig" %}
{% set data = example1|raw %}

{% set page_title = "Example 1" %}
{% set css = "example1" %}
{% set js = "example1" %}

{% block content %}
	{% embed "../library/layouts/_band.twig" %}
	
		{% block band_header %}
			|| {{ data.header }} ||
			{% for item in data.header %}
				{% embed "../library/layouts/_group.twig" with item only %}
				{% endembed %}
				{# embed "../library/components/_header.twig" with item only #}
				{# endembed #}
			{% endfor %}
		{% endblock %}
		
		{% block body %}
			{% for card in data.cards %}
				{% for side in card %}
					{% embed "../library/layouts/_card.twig" with side only %}
					{% endembed %}
				{% endfor %}
			{% endfor %}
		{% endblock %}
	{% endembed %}
	
	{% block footer %}
	{% endblock %}
	
{% endembed %}
{% endblock %}
